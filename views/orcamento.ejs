<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include("comuns/head.ejs",{title:"Orçamento"}) %>
  </head>

  <body>

    <%- include("comuns/menu.ejs",{tipo:"orcamento"}) %>


    <div class="container-fluid">
      <div class="row-fluid">
        <form action="/orcamento/cadastrar" method="post" onSubmit="return validarFormulario()">

          <table class="formulario">
            <tr>
              <td class="rotulo">Nome</td>
              <td><input type='text' id="name" name='nome'/></td>
            </tr>
            <tr>
              <td class="rotulo">Telefone</td> 
              <td><input type='text'  id="phone" name='telefone'/></td>
            </tr>
            <tr>
              <td class="rotulo">Endereço</td>
              <td><input type='text' id="address" name='endereco'/></td>
            </tr>
            <tr>
              <td class="rotulo">Email</td>
              <td><input type='text' id="email" name='email'/></td>
            </tr>
            <tr>
              <td class="rotulo">Descricao do orçamento</td>
              <td><textarea rows="10" cals="50" id="description" name="descricao"></textarea></td>
            </tr>
          </table>
          <input type='submit' name='OK' value='enviar'/>

        </form>

      </div><!--/row-->

      <hr>

      <%- include("comuns/footer") %>

    </div><!--/.fluid-container-->

    <!-- Le javascript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <%- include("comuns/javascript") %>


  </body>
</html>

